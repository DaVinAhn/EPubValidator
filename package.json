{
  "name": "EPubValidator",
  "version": "0.1.0",
  "description": "ePub Validator on Electron based.",
  "main": "main.js",
  "scripts": {
    "start": "electron --inspect=5858 main.js",
    "watch": "watchify src/app/app.jsx -t babelify -o resources/js/bundle.js --debug --verbose",
    "build": "npm run lint",
    "lint": "eslint --fix --ext .js src"
  },
  "repository": "https://github.com/DaVinAhn/EPubValidator",
  "keywords": [
    "Electron",
    "epub"
  ],
  "author": "davin.ahn <m.davinahn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.7.8",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "electron-reload": "^1.2.2",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "@ridi/eslint-config": "^3.0.0"
  }
}
